{
  "name": "ai-learning-insight-backend",
  "version": "1.0.0",
  "description": "Hapi.js backend for AI Learning Insight (Auth, Dashboard, Assessments, Predict).",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "ml:reset": "node scripts/ml_truncate.mjs",
    "ml:ingest": "node scripts/ml_ingest.mjs \"./ml/source/Dataset-Dan-Model-main\"",
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback",
    "migrate:latest": "knex migrate:latest --knexfile ./knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile ./knexfile.js",
    "ml:bootstrap": "npm run migrate:latest && node scripts/ml_ingest.mjs \"./ml/source/Dataset-Dan-Model-main\"",
    "ml:link": "node scripts/ml_link_users.mjs",
    "ml:candidates": "node scripts/ml_debug_candidates.mjs",
    "ml:debug": "node scripts/ml_debug_counts.mjs",
    "seed:mentor": "node scripts/seed_mentor.mjs",
    "bot:email": "node bot/email.js",
    "import:ml-users": "node scripts/import_ml_users_to_app.mjs"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "^21.3.10",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "3.2.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "hapi-pino": "^12.1.0",
    "joi": "^17.12.3",
    "mime-types": "^3.0.1",
    "mysql2": "^3.11.5",
    "nodemailer": "^6.9.16",
    "pg": "^8.12.0",
    "pino": "^9.4.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.5.0",
    "knex": "^3.1.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3"
  }
}
